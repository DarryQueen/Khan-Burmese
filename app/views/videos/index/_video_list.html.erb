<div class="panel panel-default module module-videos">
  <div class="panel-heading">
    <b>Videos</b>
  </div>
  <div class="panel-content">
    <table class="table">
      <thead>
        <tr>
          <th>Title</th>
          <th>Category</th>
          <th>Duration</th>
          <th>Status</th>
        </tr>
      </thead>
      <tbody>
        <% @videos.each do |video| %>
          <%= render 'videos/index/video_row', video: video %>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
