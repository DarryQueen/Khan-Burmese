$('#video-list').html("<%= escape_javascript(render('videos/index/videos_list', {
    :title => 'Videos',
    :videos => @videos,
    :message_empty => 'Nothing found!'
})) %>");

// Disable form submission advances:
$('#video-search').bind("keypress", function(e) {
  if (e.keyCode == 13) {
    return false;
  }
});
$('#search').bind("blur", function(e) {
  this.focus();
});
