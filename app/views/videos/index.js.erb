$('#video-list').html("<%= escape_javascript(render('videos/index/videos_list', :videos => @videos, :title => 'Videos')) %>");

// Disable form submission advances:
$('#video-search').bind("keypress", function(e) {
  if (e.keyCode == 13) {
    return false;
  }
});
$('#search').bind("blur", function(e) {
  this.focus();
});
